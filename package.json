{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "testing mern passport authentication",
  "main": "server.js",
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "test": "react-scripts test",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
